  "scripts": {
    "debug": "cd ./gulpfiles && node debuger.js",
    "tailwind": "npx tailwindcss -i ./src/assets/css/tailwind.css -o ./src/assets/css/main.css --watch",
    "devLimited": "gulp --isPro false",
    "devPro": "gulp",
    "liveDemoLimited": "gulp live-demo --isPro false",
    "liveDemoPro": "gulp live-demo",
    "downloadVersion": "gulp download-version --isForDownload true",
    "downloadVersionForEachTemplate": "gulp download-version-for-each-template --isForDownload true",
    "obfuscation": "gulp live-demo --isPro false && gulp temp-for-obfuscation && postcss ./.obfuscated/**/*.css --dir ./obfuscated/assets/css && gulp copy-non-pro-content",
    "copyPrelineAllToNpm": "gulp copyPrelineAllToNpm",
    "copyPrelineEachToNpm": "gulp copyPrelineEachToNpm",
    "htmlValidation": "html-validator src/.tmp --ignore-config=html-validation-ignore-config.txt",
    "htmlValidationByPath": "html-validator $SRC --ignore-config=html-validation-ignore-config.txt",
    "htmlValidationWithDelay": "html-validator src/.tmp --ignore-config=html-validation-ignore-config.txt --delay=10000",
    "htmlValidationByPathWithDelay": "html-validator $SRC --ignore-config=html-validation-ignore-config.txt --delay=10000"
  },
